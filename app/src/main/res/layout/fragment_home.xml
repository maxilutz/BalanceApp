<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mainActivity.home.HomeFragment">

    <TextView
        android:id="@+id/text_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textWeight"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:text="Poid"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textIMC"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:text="IMC"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textFatMass"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="100dp"
        android:text="taux graisse"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textMuscleMass"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="150dp"
        android:text="taux musculaire"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textWaterMass"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="200dp"
        android:text="taux d'eau"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textBPM"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="250dp"
        android:text="bpm"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/setWeight"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:text="TextView"
        android:textSize="25dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/setIMC"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:text="TextView"
        android:textSize="25dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/setFatMass"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="100dp"
        android:text="TextView"
        android:textSize="25dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/setMuscleMass"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="150dp"
        android:textAlignment="center"
        android:text="TextView"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/setWaterMass"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="200dp"
        android:text="TextView"
        android:textSize="25dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/setBPM"
        android:layout_width="205dp"
        android:layout_height="50dp"
        android:layout_marginTop="250dp"
        android:text="TextView"
        android:textSize="25dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>